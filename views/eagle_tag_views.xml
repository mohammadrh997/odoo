<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_eagle_tag_tree" model="ir.ui.view">
            <field name="name">eagle.tag.tree</field>
            <field name="model">eagle.tag</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="eagle_tags_action" model="ir.actions.act_window">
            <field name="name">Tags</field>
            <field name="res_model">eagle.tag</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>